{
  "name": "reactive-portfolio",
  "version": "1.0.0",
  "description": "This is a portfolio in react consisting of my latest web development applications.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baoxng/reactive-portfolio.git"
  },
  "author": "Bao Xiong",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "bugs": {
    "url": "https://github.com/baoxng/reactive-portfolio/issues"
  },
  "homepage": "https://github.com/baoxng/reactive-portfolio#readme",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reactstrap": "^8.9.0"
  }
}
